<script>
        $(function () {
            $.get("/Home/selectCount", "", function (res) {
                $("#yellow").html(res.count);
                $("#area").html(res.Area);
            });
            //导航栏分部视图
            $.ajax({
                url: "/Master/Index",
                data: "",
                type: "post",
                success: function (data) {
                    //data:回传的数据
                    $(".fbstone").append(data);
                    $(".aa").on("click",function () {
                        var Id1=$(this).attr("Id1");
                        var Id2=$(this).attr("Id2");
                        location.href = "/LeftNavigationBar/Home/Index?Id1=" + Id1 + "&Id2=" + Id2;
                    });
                    $(".bb").on("click", function () {
                        var Id1 = $(this).attr("Id1");
                        var Id2 = $(this).attr("Id2");
                        location.href = "/LeftNavigationBar/Home/Index?Id1=" + Id1 + "&Id2=" + Id2;
                    });
                    $(".cc").on("click", function () {
                        var Id1 = $(this).attr("Id1");
                        var Id2 = $(this).attr("Id2");
                        location.href = "/LeftNavigationBar/Home/Index?Id1=" + Id1 + "&Id2=" + Id2;
                    });
                },
                error: function (xhr) {
                    alert("错误信息" + xhr);
                    //xhr错误信息
                }
            });
            //导航栏分部视图
        })
</script>
